{
  "name": "craft-neo",
  "version": "2.0.0",
  "description": "A Matrix-like field type that uses existing fields",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjamminf/craft-neo.git"
  },
  "keywords": [
    "cms",
    "craftcms",
    "plugin",
    "neo",
    "matrix",
    "field"
  ],
  "author": "Benjamin Fleming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benjamminf/craft-neo/issues"
  },
  "homepage": "https://github.com/benjamminf/craft-neo#readme",
